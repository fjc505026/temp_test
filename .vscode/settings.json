  "C_Cpp.default.intelliSenseMode": "gcc-arm",
  "files.associations": {
    "stdbool.h": "c",
    "sll_ex.h": "c",
    "sll.h": "c",
    "dll.h": "c",
    "listack.h": "c",
    "sqlist.h": "c",
    "string.h": "c"
  },
